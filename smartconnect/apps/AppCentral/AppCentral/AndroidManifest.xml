<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.android.perf.service"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
    	<service android:name=".RemoteStub" android:process=":remote">
    		<intent-filter>
    			<!-- These are the interfaces supported by the service, which you can bind to. -->
    			<action android:name="com.remoteStub.RemoteInterface" />
    			
    			<!-- This is an action code you can use to select the service -->
    			<action android:name="com.remoteStub.REMOTE_SERVICE"/>
    		</intent-filter>
    	</service>
        <activity android:name=".RemoteStub"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>
    <uses-sdk android:minSdkVersion="6" />

</manifest> 